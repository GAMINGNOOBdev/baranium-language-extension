{
  "name": "baranium-language",
  "displayName": "baranium-language",
  "description": "baranium language support for Visual Studio Code",
  "repository": {
    "url": "https://github.com/GAMINGNOOBdev/baranium-language-extension"
  },
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.100.0"
  },
  "publisher": "GAMINGNOOBdev",
  "icon": "icon.png",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "baranium",
      "aliases": ["baranium", "baranium"],
      "extensions": [".bgs",".bar",".brn", ".ib"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "baranium",
      "scopeName": "source.baranium",
      "path": "./syntaxes/baranium.tmLanguage.json"
    }]
  }
}
